#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/sc/Documents/GitHub/webdev/node_modules/.pnpm/vuepress@2.0.0-beta.61_@vuepress+client@2.0.0-beta.61_vue@3.2.47/node_modules/vuepress/bin/node_modules:/Users/sc/Documents/GitHub/webdev/node_modules/.pnpm/vuepress@2.0.0-beta.61_@vuepress+client@2.0.0-beta.61_vue@3.2.47/node_modules/vuepress/node_modules:/Users/sc/Documents/GitHub/webdev/node_modules/.pnpm/vuepress@2.0.0-beta.61_@vuepress+client@2.0.0-beta.61_vue@3.2.47/node_modules:/Users/sc/Documents/GitHub/webdev/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/sc/Documents/GitHub/webdev/node_modules/.pnpm/vuepress@2.0.0-beta.61_@vuepress+client@2.0.0-beta.61_vue@3.2.47/node_modules/vuepress/bin/node_modules:/Users/sc/Documents/GitHub/webdev/node_modules/.pnpm/vuepress@2.0.0-beta.61_@vuepress+client@2.0.0-beta.61_vue@3.2.47/node_modules/vuepress/node_modules:/Users/sc/Documents/GitHub/webdev/node_modules/.pnpm/vuepress@2.0.0-beta.61_@vuepress+client@2.0.0-beta.61_vue@3.2.47/node_modules:/Users/sc/Documents/GitHub/webdev/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vuepress/bin/vuepress.js" "$@"
else
  exec node  "$basedir/../vuepress/bin/vuepress.js" "$@"
fi
